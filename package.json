{
    "name": "vue3-starter-kit",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "test:unit": "vue-cli-service test:unit --coverage=false",
        "lint": "vue-cli-service lint",
        "watch:css": "postcss src/assets/css/tailwind.css -o public/css/main.css",
        "start": "npm run serve",
        "beforeCommit": "eslint -c .commit.eslintrc ./src",
        "test": "jest --coverage=false"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "core-js": "^3.6.5",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "vue": "^3.0.0",
        "vue-cookies": "^1.7.4",
        "vue-router": "^4.0.0-0",
        "vuex": "^4.0.0-0",
        "vuex-persistedstate": "^4.0.0-beta.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/test-utils": "^2.0.0-0",
        "autoprefixer": "^10.2.5",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-jsdoc": "^32.3.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-vue": "^7.0.0",
        "postcss": "^8.2.8",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.2.1",
        "tailwindcss": "^2.0.4",
        "typescript": "~3.9.3",
        "vue-jest": "^5.0.0-0"
    }
}
